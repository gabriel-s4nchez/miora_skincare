{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/order\\/create\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/order\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/shop\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/cart\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/checkout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}